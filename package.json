{
  "name": "first-setup",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "format": "biome format . --write",
    "fix": "biome check --write .",
    "type-check": "tsc --noEmit",
    "better-auth:generate": "pnpx @better-auth/cli generate --output db/schemas/auth.ts",
    "drizzle:generate": "pnpm drizzle-kit generate",
    "drizzle:migrate": "pnpm drizzle-kit migrate",
    "drizzle:gm": "pnpm drizzle:generate && pnpm drizzle:migrate"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.79.0",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "drizzle-seed": "^0.3.1",
    "drizzle-zod": "^0.8.3",
    "lucide-react": "^0.552.0",
    "nanoid": "^5.1.6",
    "next": "16.0.1",
    "postgres": "^3.4.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.6",
    "supabase": "2.54.11",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
